{
  "name": "leadingsystems/contao-lscss4c",
  "description": "Leading Systems LSCSS4C as a Contao 4 bundle",
  "keywords": [
    "contao",
    "leading systems",
    "bundle"
  ],
  "type": "contao-bundle",
  "license": "proprietary",
  "require": {
    "php": "^7.0.0 || ^8.0.0 ",
    "contao/core-bundle": "~4.9",
    "leadingsystems/contao-helpers": "^2.0.0",
    "leadingsystems/lscss": "^1.0.0",
    "scssphp/scssphp": "^1.3.0"
  },
  "autoload": {
    "psr-4": {
      "LeadingSystems\\LSCSS4CBundle\\": "src/"
    },
    "classmap": [
      "src/Resources/contao/"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config/",
      "src/Resources/contao/dca/",
      "src/Resources/contao/languages/",
      "src/Resources/contao/templates/"
    ]
  },
  "extra": {
    "contao-manager-plugin": "LeadingSystems\\LSCSS4CBundle\\ContaoManager\\Plugin",
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
